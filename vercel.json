{
  "version": 2,
  "rewrites": [
    { "source": "/search", "destination": "/api/search" },
    { "source": "/proxy", "destination": "/api/proxy" }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
